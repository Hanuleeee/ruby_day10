<div class="card">
<%= form_tag("/board/#{@post.id}", method: "patch") do %> 
    <div class="card-header">
        <%= text_field_tag(:title, @post.title, class:"card-title") %>        
    </div>
    <div class="card-body">
    <%= text_area_tag(:contents, @post.contents, class:"card-text") %><br/>
    <%= submit_tag("수정하기", class:"btn btn-primary") %>
    </div>
<% end %>
</div>




