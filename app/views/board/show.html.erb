
<div class="card">
  <div class="card-header">
    <h3><%=@post.title%></h3>
  </div>
  <div class="card-body">
    <p class="card-title">작성자: <%= @post.user.user_id %></p>
    <h5 class="card-text"><%=@post.contents%></h5>
    <%= link_to "목록", "/",class:"btn btn-primary" %>
    <%= link_to "수정", "/board/#{@post.id}/edit", class:"btn btn-primary" %>
    <%= link_to "삭제", "/board/#{@post.id}", method: "delete", class:"btn btn-primary" %>
  </div>
</div>