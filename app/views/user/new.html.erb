<h2>회원가입</h2>
<%= form_tag("/user/sign_up", method: "POST") do %>
    <%= text_field_tag 'user_id',nil, placeholder: 'Enter your ID'  %><br/>
    <%= password_field_tag 'password', nil, placeholder: 'Enter your Password' %><br/>
    <%= password_field_tag 'password', nil, placeholder: 'Check your Password' %><br/>
    <%= submit_tag("Sign-Up") %>
<% end %>